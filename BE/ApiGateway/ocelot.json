{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/Movie/{scope}/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [{"Host": "movieservice", "Port": 80}],
      "UpstreamPathTemplate": "/api/Movie/{scope}/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "SwaggerKey": "movie",
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "JwtBearer",
      },
      "RouteClaimsRequirement": {
        "scope": "private"
      }
    },
    {
      "DownstreamPathTemplate": "/api/User/{scope}/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [{"Host": "userservice", "Port": 80}],
      "UpstreamPathTemplate": "/api/User/{scope}/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "SwaggerKey": "user",
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "JwtBearer",
      },
      "RouteClaimsRequirement": {
        "scope": "private"
      }
    },
    {
      "DownstreamPathTemplate": "/api/Recommendation/{scope}/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [{"Host": "recommendationservice", "Port": 80}],
      "UpstreamPathTemplate": "/api/Recommendation/{scope}/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "JwtBearer",
        "AllowedScopes": []
      },
      "RouteClaimsRequirement": {
        "scope": "private"
      }
    },
    {
      "DownstreamPathTemplate": "/api/Search/{scope}/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "searchservice",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/Search/{scope}/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "SwaggerKey": "search",
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "JwtBearer",
        "AllowedScopes": []
      },
      "RouteClaimsRequirement": {
        "scope": "private"
      }
    },
    {
      "DownstreamPathTemplate": "/api/Gmail/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "emailservice",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/Gmail/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post" ],
      "SwaggerKey": "email"
    },
    {
      "DownstreamPathTemplate": "/api/Auth/{scope}/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "authservice",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/Auth/{scope}/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "SwaggerKey": "auth",
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "JwtBearer",
        "AllowedScopes": []
      },
      "RouteClaimsRequirement": {
        "scope": "private"
      }
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "user",
      "Config": {
        "Url": "http://localhost:7228/swagger/v1/swagger.json",
        "Name": "User Service"
      }
    },
    {
      "Key": "movie",
      "Config": {
        "Url": "http://localhost:7225/swagger/v1/swagger.json",
        "Name": "Movie Service"
      }
    },
    {
      "Key": "auth",
      "Config": {
        "Url": "http://localhost:7226/swagger/v1/swagger.json",
        "Name": "Auth Service"
      }
    },
    {
      "Key": "search",
      "Config": {
        "Url": "http://localhost:7227/swagger/v1/swagger.json",
        "Name": "Search Service"
      }
    },
    {
      "Key": "email",
      "Config": {
        "Url": "http://localhost:7229/swagger/v1/swagger.json",
        "Name": "Email Service"
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000"
  }
}